
(select u1.name as results 
from MovieRating m1
join Users u1
on m1.user_id = u1.user_id 
group by m1.user_id 
order by count(distinct m1.movie_id) desc, u1.name asc
limit 1)

union all

(select m2.title
from MovieRating m1
left join Movies m2
on m1.movie_id   = m2.movie_id  
where date_format(created_at, '%Y-%m' ) = '2020-02'
group by m1.movie_id
order by avg(m1.rating) desc, m2.title asc
limit 1
)
